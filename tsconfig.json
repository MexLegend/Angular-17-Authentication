/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "compileOnSave": false,
  "compilerOptions": {
    "outDir": "./dist/out-tsc",
    "baseUrl": "./",
    "paths": {
      "@env/advanced-auth/*": [
        "projects/advanced-oauth2-oidc-open-id/src/environments/*"
      ],
      "@core/advanced-auth/*": [
        "projects/advanced-oauth2-oidc-open-id/src/app/core/*"
      ],
      "@shared/advanced-auth/*": [
        "projects/advanced-oauth2-oidc-open-id/src/app/shared/*"
      ],
      "@env/google-gsi-client/*": [
        "projects/google-gsi-client/src/environments/*"
      ],
      "@core/google-gsi-client/*": [
        "projects/google-gsi-client/src/app/core/*"
      ],
      "@shared/google-gsi-client/*": [
        "projects/google-gsi-client/src/app/shared/*"
      ],
      "@env/google-auth2-gapi/*": [
        "projects/google-auth2-gapi/src/environments/*"
      ],
      "@core/google-auth2-gapi/*": [
        "projects/google-auth2-gapi/src/app/core/*"
      ],
      "@shared/google-auth2-gapi/*": [
        "projects/google-auth2-gapi/src/app/shared/*"
      ],
      "@env/firebase-auth/*": [
        "projects/firebase-auth/src/environments/*"
      ],
      "@core/firebase-auth/*": [
        "projects/firebase-auth/src/app/core/*"
      ],
      "@shared/firebase-auth/*": [
        "projects/firebase-auth/src/app/shared/*"
      ]
    },
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitOverride": true,
    "noPropertyAccessFromIndexSignature": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "declaration": false,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "ES2022",
    "module": "ES2022",
    "useDefineForClassFields": false,
    "lib": ["ES2022", "dom"]
  },
  "angularCompilerOptions": {
    "enableI18nLegacyMessageIdFormat": false,
    "strictInjectionParameters": true,
    "strictInputAccessModifiers": true,
    "strictTemplates": true
  }
}
